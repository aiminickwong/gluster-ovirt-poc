<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:d="urn:import:org.ovirt.engine.ui.webadmin.widget.dialog"
	xmlns:e="urn:import:org.ovirt.engine.ui.webadmin.widget.editor"
	xmlns:t="urn:import:org.ovirt.engine.ui.webadmin.widget.dialog.tab">

	<ui:style>
		.searchItemsPanel {
			width: 100%;
			height: 77%;
			border-bottom: 1px solid #BBBBBB;
			border-top: 1px solid #BBBBBB;
		}
	</ui:style>

	<d:SimpleDialogPanel width="400px" height="300px">
		<d:content>
			<g:ScrollPanel addStyleNames="{style.searchItemsPanel}">
				<e:EntityModelCellTable ui:field="brickList" />
			</g:ScrollPanel>
		</d:content>
	</d:SimpleDialogPanel>
</ui:UiBinder>